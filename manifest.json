{
    "description": "Cancels downloads",
    "manifest_version": 2,
    "name": "cancel-downloads",
    "version": "1.0",
    "developer": {
        "name": "cjlangan",
        "url": "https://github.com/cjlangan/webextension-cancel-downloads"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{a762ee65-eb4f-4ead-aa9b-bc91baf422fe}",
            "strict_min_version": "79.0"
        }
    },
    "permissions": [
        "downloads"
    ],
    "browser_action": {
        "default_title": "Cancel All Downloads",
        "default_icon": "icons/cancel-duplicate-downloads.svg"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
